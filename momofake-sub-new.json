{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      { "tag": "proxy-dns", "address": "https://8.8.8.8/dns-query", "detour": "proxy" },
      { "tag": "local-dns", "address": "223.5.5.5", "detour": "direct" },
      { "tag": "block-dns", "address": "rcode://success" }
    ],
    "rules": [
      { "outbound": "any", "server": "local-dns" },
      { "clash_mode": "Direct", "server": "local-dns" },
      { "clash_mode": "Global", "server": "proxy-dns" },
      { "rule_set": "geosite-category-ads-all", "server": "block-dns" },
      { "rule_set": ["geosite-youtube", "geosite-google", "geosite-telegram", "geosite-twitter"], "server": "proxy-dns" },
      { "rule_set": "geosite-cn", "server": "local-dns" }
    ],
    "final": "proxy-dns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "inet4_address": "172.19.0.1/30",
      "mtu": 1400,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "sniff": true,
      "sniff_override_destination": true,
      "udp_timeout": 300
    }
  ],
  "outbounds": [
    { "type": "selector", "tag": "proxy", "outbounds": ["{all}"] },
    { "type": "selector", "tag": "google", "outbounds": ["proxy", "direct"] },
    { "type": "selector", "tag": "telegram", "outbounds": ["proxy", "direct"] },
    { "type": "selector", "tag": "twitter", "outbounds": ["proxy", "direct"] },
    { "type": "direct", "tag": "direct" },
    { "type": "dns", "tag": "dns-out" },
    { "type": "block", "tag": "block" }
  ],
  "route": {
    "rule_set": [
      { "tag": "geosite-category-ads-all", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs" },
      { "tag": "geosite-youtube", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-youtube.srs" },
      { "tag": "geosite-google", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-google.srs" },
      { "tag": "geosite-telegram", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-telegram.srs" },
      { "tag": "geosite-twitter", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-twitter.srs" },
      { "tag": "geosite-cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs" },
      { "tag": "geoip-cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs" }
    ],
    "rules": [
      { "protocol": "dns", "outbound": "dns-out" },
      { "clash_mode": "Direct", "outbound": "direct" },
      { "clash_mode": "Global", "outbound": "proxy" },
      { "rule_set": "geosite-category-ads-all", "outbound": "block" },
      { "rule_set": "geosite-youtube", "outbound": "google" },
      { "rule_set": "geosite-google", "outbound": "google" },
      { "rule_set": "geosite-telegram", "outbound": "telegram" },
      { "rule_set": "geosite-twitter", "outbound": "twitter" },
      { "rule_set": "geosite-cn", "outbound": "direct" },
      { "rule_set": "geoip-cn", "outbound": "direct" },
      { "ip_is_private": true, "outbound": "direct" }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  },
  "experimental": {
    "cache_file": { "enabled": true, "path": "cache.db" },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "ui",
      "default_mode": "Rule"
    }
  }
}
